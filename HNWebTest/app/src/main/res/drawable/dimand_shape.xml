<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:width="30dp"
                    android:height="30dp" />
                <gradient
                    android:startColor="#ca8911"
                    android:endColor="#ca8911" />
                <stroke
                    android:width="5dp"
                    android:color="#ca8911" />
            </shape>
        </rotate>
    </item>
</layer-list>